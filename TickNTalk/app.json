{
  "expo": {
    "name": "TickNTalk",
    "slug": "TickNTalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/images/Logo.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.TickNTalkCoop.TickNTalk",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.TickNTalkCoop.TickNTalk",
      "versionCode": 1,
      "adaptiveIcon" : {
        "foregroundImage" : "./assets/images/Logo.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
